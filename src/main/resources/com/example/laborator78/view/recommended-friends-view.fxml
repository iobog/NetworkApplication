<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="@../css/whatsapp.css"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.laborator78.controller.RecommendedFriendsController"
            prefHeight="400.0" prefWidth="600.0">

    <AnchorPane prefHeight="314.0" prefWidth="663.0">
        <TableView fx:id="tableView" minHeight="-Infinity" minWidth="-Infinity" prefHeight="315.0" prefWidth="663.0" AnchorPane.bottomAnchor="66.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="tableColumnFirstName" prefWidth="75.0" text="First Name" />
                <TableColumn fx:id="tableColumnLastName" prefWidth="75.0" text="Last Name" />

            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Button fx:id="buttonSendRequest" layoutX="514.0" layoutY="329.0" onAction="#onSendRequestButtonClick" style="-fx-font-size: 14px;" text="Send request" />
        <Button layoutX="455.0" layoutY="329.0" onAction="#onBackButtonClick" style="-fx-font-size: 14px;" text="Back" />
    </AnchorPane>
    <HBox />
<!--    <Button layoutX="425.0" layoutY="323.0" onAction="#onAddFriendButtonClick" style="-fx-font-size: 14px;" text="Add Friend" />-->

</AnchorPane>
