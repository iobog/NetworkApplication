        </TableView>
        <Button fx:id="buttonDeleteRequest" layoutX="514.0" layoutY="329.0" onAction="#onDeleteButtonClick" style="-fx-font-size: 14px;" text="Delete request" />
        <Button layoutX="455.0" layoutY="329.0" onAction="#onBackButtonClick" style="-fx-font-size: 14px;" text="Back" />
    </AnchorPane>

</AnchorPane>
